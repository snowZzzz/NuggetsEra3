<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nuggetsera">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/login_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity" />
        <activity android:name=".ui.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.LoginActivity" />
        <activity android:name=".ui.RegisteredActivity" />
        <activity android:name=".ui.RegisteredNextActivity" />
        <activity android:name=".ui.ChangePasswordActivity" />
        <activity android:name=".ui.ChangePasswordTwoActivity" />
        <activity android:name=".ui.MyActivity" />
        <activity android:name=".ui.TestBtnActivity"></activity>
    </application>

</manifest>